# Nano-banana MCP Configuration
# Copy this file to .env and add your API key

# Your Gemini API key from Google AI Studio
# Get it from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Azure Blob Storage connection string
# Get it from Azure Portal -> Storage Account -> Access keys
AZURE_STORAGE_CONNECTION_STRING=your_azure_storage_connection_string_here

# Azure Blob Storage container name (optional, defaults to 'nano-banana-images')
AZURE_STORAGE_CONTAINER_NAME=nano-banana-images